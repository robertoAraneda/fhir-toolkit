    "clean": "rm -rf dist"
  },
  "keywords": [
    "fhir",
    "r4",
    "typescript",
    "types",
    "healthcare"
  ],
  "author": "Roberto Araneda",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
