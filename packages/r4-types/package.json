{
  "name": "@fhir-toolkit/r4-types",
  "version": "0.1.1",
  "description": "FHIR R4 TypeScript type definitions and valuesets",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
