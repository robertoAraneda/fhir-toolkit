{
  "name": "@fhir-toolkit/r4-models",
  "version": "0.2.0",
  "description": "FHIR R4 models and builders for TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "fhir",
    "r4",
    "hl7",
    "healthcare",
    "typescript",
    "models",
    "builders"
  ],
  "author": "Roberto Araneda <robaraneda@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertoAraneda/fhir-toolkit.git",
    "directory": "packages/r4-models"
  },
  "bugs": {
    "url": "https://github.com/robertoAraneda/fhir-toolkit/issues"
  },
  "dependencies": {
    "@fhir-toolkit/r4-types": "workspace:*"
  },
  "peerDependencies": {
    "@fhir-toolkit/yafv": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@fhir-toolkit/yafv": {
      "optional": true
    }
  },
  "devDependencies": {
    "@fhir-toolkit/yafv": "workspace:*",
    "tsup": "^8.0.2",
    "vitest": "^1.4.0"
  },
  "sideEffects": false
}
